{
  "name": "print-response",
  "version": "1.0.0",
  "description": "Terminal output of Fastify http response received by .inject() command",
  "main": "dist/print-response.js",
  "scripts": {
    "demo": "nodemon --exec ts-node --files ./demo/main.ts",
    "fserv": "nodemon --exec ts-node --files ./demo/fserv.ts",
    "build": "rimraf ./dist && tsc --project tsconfig.build.json",
    "test": "jest --verbose --silent --detectOpenHandles --forceExit",
    "happydev": "nodemon --exec ts-node src/happydev/tools/dev.ts"
  },
  "keywords": [
    "terminal",
    "console",
    "beautify",
    "print",
    "HTTP",
    "Fastify",
    "inject"
  ],
  "author": "mkant@list.ru",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-kant/print-response"
  },
  "dependencies": {
    "obj-console": "^1.0.2"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.5",
    "chalk": "^5.2.0",
    "fastify": "^4.14.0",
    "ip": "^1.1.8",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "nodemon": "^2.0.21",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
